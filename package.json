{
  "name": "@donkeyclip/pixi-displacement-filter",
  "version": "2.0.0",
  "description": "Pixi Displacement Filter plugin for motorcortex",
  "main": "dist/pixi-displacement-filter.cjs.js",
  "module": "dist/pixi-displacement-filter.esm.js",
  "browser": "dist/pixi-displacement-filter.umd.js",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/pixi-displacement-filter"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --mode=development --config ./demo/webpack.config.js",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">= 8 < 10"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@babel/plugin-syntax-jsx": "7.21.4",
    "@babel/plugin-transform-react-jsx": "7.21.5",
    "@babel/preset-env": "7.21.5",
    "@donkeyclip/motorcortex": "^9.6.0",
    "@donkeyclip/motorcortex-player": "2.10.8",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@size-limit/preset-big-lib": "6.0.4",
    "babel-loader": "8.3.0",
    "concurrently": "6.5.1",
    "core-js": "3.30.2",
    "css-loader": "6.7.4",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "5.2.0",
    "husky": "7.0.4",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "size-limit": "6.0.4",
    "webpack": "5.83.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.0"
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "pixi.js": "^7.2.4"
  }
}
